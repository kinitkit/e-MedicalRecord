                                    android:textSize="@dimen/sp_17" />
                            </android.support.design.widget.TextInputLayout>

                            <android.support.design.widget.TextInputLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textAppearance="@style/TextAppearance.AppCompat.Title"
                                android:textColorHint="@color/colorPrimaryDark"
                                android:textSize="17sp">

                                <android.support.design.widget.TextInputEditText
                                    android:id="@+id/et_hyaline"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:hint="Hyaline"
                                    android:inputType="text"
                                    android:maxLength="20"
                                    android:paddingTop="@dimen/dp_3"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                                    android:textColor="@color/colorTextPrimary"
                                    android:textSize="@dimen/sp_17" />
                            </android.support.design.widget.TextInputLayout>

                            <android.support.design.widget.TextInputLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textAppearance="@style/TextAppearance.AppCompat.Title"
                                android:textColorHint="@color/colorPrimaryDark"
                                android:textSize="17sp">

                                <android.support.design.widget.TextInputEditText
                                    android:id="@+id/et_fineGranular"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:hint="Fine Granular"
                                    android:inputType="text"
                                    android:maxLength="20"
                                    android:paddingTop="@dimen/dp_3"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                                    android:textColor="@color/colorTextPrimary"
                                    android:textSize="@dimen/sp_17" />
                            </android.support.design.widget.TextInputLayout>

                            <android.support.design.widget.TextInputLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textAppearance="@style/TextAppearance.AppCompat.Title"
                                android:textColorHint="@color/colorPrimaryDark"
                                android:textSize="17sp">

                                <android.support.design.widget.TextInputEditText
                                    android:id="@+id/et_coarseGranular"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:hint="Coarse Granular"
                                    android:inputType="text"
                                    android:maxLength="20"
                                    android:paddingTop="@dimen/dp_3"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                                    android:textColor="@color/colorTextPrimary"
                                    android:textSize="@dimen/sp_17" />
                            </android.support.design.widget.TextInputLayout>

                            <android.support.design.widget.TextInputLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textAppearance="@style/TextAppearance.AppCompat.Title"
                                android:textColorHint="@color/colorPrimaryDark"
                                android:textSize="17sp">

                                <android.support.design.widget.TextInputEditText
                                    android:id="@+id/et_remarks"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:hint="Remarks"
                                    android:inputType="textMultiLine"
                                    android:maxLines="3"
                                    android:paddingTop="@dimen/dp_3"
                                    android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                                    android:textColor="@color/colorTextPrimary"
                                    android:textSize="@dimen/sp_17" />
                            </android.support.design.widget.TextInputLayout>
                        </LinearLayout>
                    </android.support.v7.widget.CardView>

                    <android.support.v7.widget.AppCompatButton
                        android:id="@+id/btn_save"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/colorAccent"
                        android:padding="12dp"
                        android:text="@string/save"
                        android:textAppearance="@style/TextAppearance.AppCompat.Button"
                        android:textColor="#ffffff" />
                </LinearLayout>
            </android.support.v4.widget.NestedScrollView>

            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                card_view:cardCornerRadius="0dp">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#ffffff"
                    android:paddingLeft="@dimen/dp_5"
                    android:paddingRight="@dimen/dp_5">

                    <TextView
                        android:id="@+id/tv_goTo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="Go to:"
                        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                        android:textColor="@color/colorTextSecondary"
                        android:textSize="17sp" />

                    <android.support.v7.widget.AppCompatSpinner
                        android:id="@+id/spinner_lab_search"
                        style="@style/Base.Widget.AppCompat.Spinner.Underlined"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dp_10"
                        android:layout_toRightOf="@+id/tv_goTo"
                        android:padding="0dp"
                        android:spinnerMode="dialog"
                        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                        android:textColor="@color/colorTextSecondary"
                        android:textSize="17sp" />
                </RelativeLayout>
            </android.support.v7.widget.CardView>
        </android.support.design.widget.CoordinatorLayout>
    </LinearLayout>
</RelativeLayout>
